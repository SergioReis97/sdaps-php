#!/usr/bin/make -f

# This file was automatically generated by stdeb 0.6.0+git at
# Sun, 17 Feb 2013 12:59:45 +0100

%:
	dh $@ --with python2

override_dh_clean:
	rm -rf build/mo
	# Remove the pot files (yeah, this is probably not the right
	# solution)
	rm -f po/sdaps.pot
	rm -f tex/po/sdaps-tex.pot
	# And the test directory
	rm -rf test/projects
	dh_clean

override_dh_auto_build:
	dh_auto_build

	# And run the testsuit
ifeq (,$(filter nocheck,$(DEB_BUILD_OPTIONS)))
	cd test; ./run-test-locally.sh
endif
